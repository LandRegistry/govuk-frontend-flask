{
  "name": "govuk-frontend-flask",
  "version": "1.0.0",
  "description": "GOV.UK Frontend Flask",
  "private": true,
  "scripts": {
    "format:check": "prettier src --check",
    "format:fix": "prettier src --write",
    "lint:check": "eslint src",
    "lint:fix": "eslint src --fix",
    "prebuild": "npm run format:check && npm run lint:check",
    "build": "webpack",
    "watch": "webpack --watch",
    "upgrade:latest": "ncu -u",
    "upgrade:minor": "ncu -t minor -u",
    "upgrade:patch": "ncu -t patch -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LandRegistry/govuk-frontend-flask.git"
  },
  "author": {
    "name": "Matt Shaw",
    "url": "https://matthew-shaw.github.io/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LandRegistry/govuk-frontend-flask/issues"
  },
  "homepage": "https://github.com/LandRegistry/govuk-frontend-flask#readme",
  "dependencies": {
    "govuk-frontend": "5.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.38.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "mini-css-extract-plugin": "^2.9.4",
    "npm-check-updates": "^19.1.1",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
